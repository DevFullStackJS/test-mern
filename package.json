{
  "name": "test-rest-api",
  "version": "1.0.0",
  "description": "REST Api pour l'application mern",
  "main": "index.js",
  "repository": {
    "type": "Subversion",
    "directory": "test-mern",
    "url": "https://github.com/DevFullStackJS/test-mern"
  },
  "scripts": {
    "start": "npm run seed:local && nodemon",
    "build": "tsc --build",
    "deploy:local": "NODE_ENV=local-deploy pm2 start pm2.config.js",
    "deploy:dev": "npm run seed:dev && NODE_ENV=dev pm2 start pm2.config.js",
    "deploy:qualif": "npm run seed:qualif && NODE_ENV=qualif pm2 start pm2.config.js",
    "stop": "pm2 stop backend",
    "seed:config": "ts-node -r dotenv-flow/config ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node -r dotenv-flow/config ./node_modules/typeorm-seeding/dist/cli.js seed",
    "seed:local": "NODE_ENV=local ts-node -r dotenv-flow/config ./node_modules/typeorm-seeding/dist/cli.js seed",
    "seed:dev": "NODE_ENV=dev ts-node -r dotenv-flow/config ./node_modules/typeorm-seeding/dist/cli.js seed",
    "seed:qualif": "NODE_ENV=qualif ts-node -r dotenv-flow/config ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "keywords": [
    "express",
    "passport",
    "jwt",
    "typeorm",
    "socket.io"
  ],
  "author": "DevFullStackJS",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/multer": "^1.4.5",
    "@types/node-cron": "^2.0.3",
    "@types/nodemailer": "^6.4.1",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dayjs": "^1.10.4",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "express-jsdoc-swagger": "^1.4.0",
    "helmet": "^4.4.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.9",
    "morphism": "^1.12.3",
    "multer": "^1.4.2",
    "node-cron": "^3.0.0",
    "node-mocks-http": "^1.10.1",
    "nodemailer": "^6.5.0",
    "nodemailer-express-handlebars": "^4.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.0.1",
    "typeorm": "^0.2.32",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
